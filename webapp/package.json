{
  "name": "super-terrific-happy-app",
  "version": "0.0.1",
  "description": "This is the Super Terrific Happy App",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:your-name-here/super-terrific-happy-app.git"
  },
  "author": "Your Name Here",
  "license": "ISC",
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "hammerjs": "./app/components/hammerjs/hammer.js",
    "materialize-js": "./node_modules/materialize-css/bin/materialize.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "node-underscorify",
        {
          "extensions": [
            "jst",
            "ejs"
          ],
          "requires": [
            {
              "variable": "_",
              "module": "lodash"
            }
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "materialize-js": {
      "exports": "Materialize",
      "depends": [
        "jquery",
        "hammerjs"
      ]
    }
  },
  "devDependencies": {
    "backbone": "~1.1.2",
    "bootstrap": "^3.3.2",
    "browser-sync": "^2.7.1",
    "browserify": "^10.2.0",
    "browserify-shim": "^3.8.7",
    "del": "^1.1.1",
    "font-awesome": "^4.3.0",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^0.4.1",
    "gulp-filesize": "0.0.6",
    "gulp-imagemin": "^0.6.2",
    "gulp-less": "^3.0.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-notify": "^1.4.2",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^2.0.4",
    "gulp-sftp": "^0.1.4",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-uglify": "^1.0.2",
    "gulp-util": "^3.0.0",
    "jquery": "~2.1.0",
    "jquery-ui": "^1.10.5",
    "keymaster": "^1.6.2",
    "lodash": "^2.4.1",
    "node-underscorify": "0.0.14",
    "pretty-hrtime": "~0.2.1",
    "proxy-middleware": "^0.9.0",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "^2.2.1",
    "yargs": "^3.7.0"
  },
  "dependencies": {
    "materialize-css": "^0.97.1"
  }
}
